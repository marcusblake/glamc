func fib(n: int) int {
    lst := [0, 1];
    i := 2;
    while (i <= n) {
        append(lst, lst[i-1] + lst[i-2]);
        i = i + 1;
    }
    return lst[n];
}


func main() {
    id := 0;
    var fibs list<int>;
    while (id <= 20) {
        append(fibs, fib(id));
        id = id + 1;
    }

    for (num in fibs) {
        println(num);
    }
}