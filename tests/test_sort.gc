func swap(li: list<int>, j: int, i: int) {
    temp := li[j];
    li[j] = li[i];
    li[i] = temp;
}


func selection_sort(a: list<int>) list<int> {
    i := 0;
    n := len(a);

    while (i < n) {
        j := i;
        min_left := 10000000;
        idx := i;
        while (j < n) {
            if (a[j] < min_left) {
                idx = j;
                min_left = a[j];
            }
            j += 1;
        }
        swap(a, i, idx);
        i += 1;
    }
    return a;
}


func main() {
    result := selection_sort([-1, 4 , 0, 12, 44, 30, 99, -6, 33, 23, 66, 10]);
    sorted := [-6, -1, 0, 4, 10, 12, 23, 30, 33, 44, 66, 99];

    if (len(result) != len(sorted)) {
        println("result and sorted have unequal lengths");
    }

    var length int = len(result);
    for (index in 0...length) {
        if (result[index] != sorted[index]) {
            println("result and sorted have unequal values");
        }
    }
}