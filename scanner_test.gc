func is_palindrome(string k) bool {
    j := lenstr(k) - 1;
    i := 0;
    bool is_palindrome = true;
    while (i <= j) {
        if (k[i] != k[j]) {
            is_palindrome = false;
        }
        i = i + 1;
        j = j - 1;
    }
    return is_palindrome;
}


