func main() int {
    list<string> words = ["Marcus Blake", "racecar", "dogeeseseegod", "datyayyay"];
    count := 0;
    list<string> palindromes = [" "];
    while (count < 4) {
        str := words[count];
        if (is_palindrome(str)) {
            append(palindromes, str);
        }
        count = count + 1;
    }
    printStringList(palindromes);
    
    id := lenlist(palindromes);
    count = 0;
    while (count < id) {
        prints(palindromes[count]);
        count = count + 1;
    }
    return 0;
}